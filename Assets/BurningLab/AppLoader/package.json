{
  "name": "com.burning-lab.apploader",
  "version": "0.0.1",
  "displayName": "App Loader",
  "description": "Application loading pipeline controller for unity projects.",
  "unity": "2021.3",
  "documentationUrl": "",
  "changelogUrl": "https://github.com/burning-laboratory/unity-app-loader/releases",
  "licensesUrl": "https://github.com/burning-laboratory/unity-app-loader/blob/main/LICENSE.txt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/burning-laboratory/unity-app-loader"
  },
  "keywords": [
    "Unity", "App", "Loader"
  ],
  "scripts": {
    "build": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/ReadmeBuilder.js",
    "set-suffix": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/ReleaseSuffixSetter.js",
    "clear-suffix": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/ClearVersionSuffix.js",
    "cache-dependencies": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/DependenciesSaver.js",
    "load-dependencies": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/DependenciesLoader.js",
    "restore-dependencies": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/DependencyRestore.js",
    "process-samples": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/SamplesProcessor.js",
    "restore-samples": "node ./node_modules/com.burning-lab.utils.js-build-utils/Bin/SamplesRestore.js"
  },
  "author": {
    "name": "Burning-Lab",
    "email": "packages.support@burning-lab.com",
    "url": "https://burning-lab.com"
  },
  "devDependencies": {
    "com.burning-lab.utils.js-build-utils": "^0.1.12"
  },
  "samples": [
    {
      "displayName": "Simple App With Preloader",
      "description": "Simple application with asset implementation.",
      "path": "Samples/Simple App"
    }
  ]
}